{
  "name": "@salesforce/plugin-templates",
  "version": "55.0.0",
  "main": "index.js",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/salesforcedx-templates/issues",
  "dependencies": {
    "@oclif/core": "^1.12.0",
    "@salesforce/command": "^5.2.1",
    "@salesforce/core": "^3.25.1",
    "@salesforce/templates": "55.0.0",
    "tslib": "^1",
    "yeoman-environment": "^3.9.1",
    "yeoman-generator": "^5.6.1"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.1.3",
    "@oclif/plugin-help": "^5.1.12",
    "@salesforce/dev-scripts": "^2.0.4",
    "@salesforce/dev-config": "^3.0.0",
    "@salesforce/plugin-command-reference": "^1.3.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.2.2",
    "@types/mkdirp": "^0.5.2",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^5.2.9",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "chai": "^4.2.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "oclif": "^3.1.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "shx": "^0.3.4",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.4.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.3.0"
  },
  "resolutions": {
    "yeoman-generator": "^5.6.1",
    "@types/yeoman-generator": "^5.2.9"
  },
  "homepage": "https://github.com/forcedotcom/salesforcedx-templates",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "yarn.lock",
    "/bin",
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "force": {
        "external": true,
        "subtopics": {
          "apex": {
            "external": true
          },
          "analytics": {
            "description": "work with analytics assets",
            "longDescription": "Work with analytics assets.",
            "subtopics": {
              "template": {
                "description": "work with analytics templates",
                "longDescription": "Work with analytics templates."
              }
            }
          },
          "lightning": {
            "external": true
          },
          "project": {
            "external": true,
            "subtopics": {}
          },
          "visualforce": {
            "external": true,
            "subtopics": {}
          },
          "staticresource": {
            "external": true,
            "subtopics": {}
          }
        }
      }
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": "forcedotcom/salesforcedx-templates",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "manifest:generate": "oclif manifest",
    "manifest:remove": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:command-reference",
    "prepack": "sf-prepack",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate -p @salesforce/plugin-templates --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "version": "oclif readme && git add README.md"
  }
}
